<struts>
    
    <package name="struts-validateCode" namespace="/validates" extends="struts-default">
        <action name="showForm" class="com.lxp.web.action.ValidateCodeAction" method="showForm">
        	<result>/WEB-INF/views/validate/showForm.jsp</result>
